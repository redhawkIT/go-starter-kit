  "description": "",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "olebedev",
  "license": "MIT",
  "dependencies": {
    "autoprefixer": "^6.2.3",
    "autoprefixer-loader": "^3.1.0",
    "babel-core": "^5.4.7",
    "babel-eslint": "^3.1.9",
    "babel-loader": "^5.1.2",
    "babel-plugin-react-transform": "^1.1.1",
    "css-loader": "^0.16.0",
    "cssrecipes-defaults": "^0.5.0",
    "eslint-plugin-react": "^2.6.4",
    "expose-loader": "^0.7.0",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^0.8.1",
    "file-loader": "^0.8.4",
    "flummox": "^3.5.2",
    "history": "^1.13.0",
    "lodash": "^3.10.0",
    "normalize.css": "^3.0.3",
    "postcss-functions": "^2.0.0",
    "postcss-loader": "^0.8.0",
    "precss": "^1.4.0",
    "proxy-middleware": "^0.15.0",
    "react": "^0.14.1",
    "react-dom": "^0.14.1",
    "react-helmet": "^1.1.3",
    "react-redux": "^4.0.0",
    "react-router": "^1.0.0-rc3",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.1.1",
    "redux": "^3.0.4",
    "redux-devtools": "^2.1.5",
    "style-loader": "^0.12.3",
    "stylus-loader": "1.2.1",
    "webpack": "^1.9.6",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.4.1",
    "whatwg-fetch": "^0.9.0",
    "when": "^3.7.3"
  },
  "babel": {
    "stage": 0,
    "env": {
      "dev": {
        "plugins": [
          "react-transform"
        ],
        "extra": {
          "react-transform": {
            "transforms": [
              {
                "transform": "react-transform-hmr",
                "imports": [
                  "react"
                ],
                "locals": [
                  "module"
                ]
              },
              {
                "transform": "react-transform-catch-errors",
                "imports": [
                  "react",
                  "redbox-react"
                ]
              }
            ]
          }
        }
      }
    }
  },
  "eslintConfig": {
    "rules": {
      "indent": [
        2,
        2
      ],
      "quotes": [
        2,
        "single"
      ],
      "linebreak-style": [
        2,
        "unix"
      ],
      "semi": [
        2,
        "always"
      ],
      "react/jsx-uses-react": 2,
      "react/jsx-uses-vars": 2,
      "react/react-in-jsx-scope": 2
    },
    "env": {
      "es6": true,
      "browser": true,
      "node": true
    },
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    },
    "plugins": [
      "react"
    ]
  }
}
